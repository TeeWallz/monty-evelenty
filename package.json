{
  "name": "monty-evelenty",
  "version": "1.0.0",
  "description": "Source https://css-tricks.com/eleventy-starter-with-tailwind-css-alpine-js/",
  "main": "index.js",
  "scripts": {
    "start": "eleventy --serve & npx tailwindcss -i styles/tailwind.css -c styles/tailwind.config.js -o _site/style.css --watch",
    "build": "ELEVENTY_PRODUCTION=true eleventy && NODE_ENV=production npx tailwindcss -i styles/tailwind.css -c styles/tailwind.config.js -o _site/style.css --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeeWallz/monty-evelenty.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TeeWallz/monty-evelenty/issues"
  },
  "homepage": "https://github.com/TeeWallz/monty-evelenty#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3",
    "alpinejs": "^3.12.2",
    "html-minifier": "^4.0.0",
    "md5-file": "5.0.0",
    "tailwindcss": "^3.3.2"
  }
}
